{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "Pet adoption app using React.JS",
  "main": "index.js",
  "scripts": {
    "prebuild": "run-s clean",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --no-minify --public-url /",
    "clean": "rimraf ./dist",
    "start": "cross-env NODE_ENV=development parcel src/index.html ",
    "dev": "parcel src/index.html",
    "dev:mock": "cross-env PET_MOCK=mock parcel src/index.html",
    "format": "prettier \"src/**/*.{js,html,jsx}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hussnainwithss/adopt-me.git"
  },
  "keywords": [
    "React",
    "JS",
    "Javascript",
    "Frontend"
  ],
  "author": "Hussnain Ahmad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hussnainwithss/adopt-me/issues"
  },
  "homepage": "https://github.com/hussnainwithss/adopt-me#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@frontendmasters/pet": "^1.0.3",
    "@reach/router": "^1.3.3",
    "babel-polyfill": "^6.26.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "autoprefixer": "^7.2.1",
    "babel-preset-react-app": "^3.1.0",
    "parcel-bundler": "latest"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
    "last 2 Edge versions"
  ]
}
